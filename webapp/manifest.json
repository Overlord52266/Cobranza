{
    "_version": "1.42.0",
    "sap.app": {
        "id": "cobranza",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.1",
            "toolsId": "98adf5ee-3892-467c-812b-b5d8de6dae31"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "cobranza-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "cobranza",
                    "action": "Display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://loan"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cobranza.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "Reporte": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "MedioPago": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Status": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "BancoCuenta": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Banco": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Cuenta": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "UserLogin": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Planilla": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "DetallePlanilla": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Cliente": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Archivos": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            },
            "Proyect": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "cobranza.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": [
                        "TargetMenu"
                    ]
                }
                
            },
            "routes": [
                {
                    "name": "RouteMenu",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMenu"
                    ]
                },
                {
                    "name": "RoutePlanillaView1",
                    "pattern": "RoutePlanillaView1/:context:",
                    "target": [
                        "TargetPlanillaView1"
                    ]
                },
                {
                    "name": "RoutePlanillaView2",
                    "pattern": "RoutePlanillaView2/:context:",
                    "target": [
                        "TargetPlanillaView2"
                    ]
                },
                {
                    "name": "RouteReporteView",
                    "pattern": "RouteReporteView/:context:",
                    "target": [
                        "TargetReporteView"
                    ]
                }
            ],
            "targets": {
                "TargetMenu": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Menu",
                    "viewName": "Menu"
                },
                "TargetPlanillaView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "PlanillaView1",
                    "viewName": "PlanillaView1"
                },
                "TargetPlanillaView2": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "PlanillaView2",
                    "viewName": "PlanillaView2"
                },
                "TargetReporteView": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ReporteView",
                    "viewName": "ReporteView"
                }
            }
        },
        "rootView": {
            "viewName": "cobranza.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
          }
       
    },
    "sap.cloud": {
        "public": true,
        "service": "cobranza"
    }
}