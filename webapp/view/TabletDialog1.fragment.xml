		<columns>
			<Column demandPopin="true" hAlign="Center" width="auto">
				<header>
					<Text text="Documento" />
				</header>
			</Column>

            <Column demandPopin="true" hAlign="Center" width="auto">
				<header>
					<Text text="Tipo Doc." />
				</header>
			</Column>

			<Column
				minScreenWidth="Desktop"
                hAlign="Center"
				demandPopin="true"
                width="auto">
				<header>
					<Text text="RUC/DNI" />
