    <TableSelectDialog
		id="TableDialogDoc"
		noDataText="No Products Found"
		title="Seleccionar Documento"
		search=".handleSearch"
        items="{Reporte>/data}"
		confirm=".handleConfirm"
		cancel=".handleClose"
        multiSelect="true"
